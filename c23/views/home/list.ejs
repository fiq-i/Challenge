<%- include('../partials/header') %>

<form action="" method="GET">
  <input type="hidden" name="page" value="1" />
  
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark" >
    <div class="container-fluid">
      <a  class="navbar-brand" href="#">Project Management System</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a  class="nav-link active" aria-current="page" href="#">Projects</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="profile">Profile</a>
          </li>
          
        </ul>
      </div>
    </div>
  </nav>


  <br>
  <h1>Projects</h1>

  <input onclick="myFunction1()" type="checkbox" id="myCheck1" />
  <div class="mb-3 row">
    <label for="ID" class="col-sm-2 col-form-label">ID</label >
    <div class="col-sm-10">
      <input
        type="text"
       
        class="form-control"
        id="ID"
        name="id"
        placeholder="ID"
        disabled
      />
    </div>
  </div>

  

  <input onclick="myFunction2()" type="checkbox" id="myCheck2">
  <div class="mb-3 row">
    <label for="Name" class="col-sm-2 col-form-label">Name</label >
    <div class="col-sm-10">
      <input
        type="number"
        
        class="form-control"
        id="Name"
        name="name"
        placeholder="Name"
        disabled
      />
    </div>
  </div>

  
  <input onclick="myFunction3()" type="checkbox" id="myCheck3">
  <div class="mb-3 row">
    <label for="Member" class="col-sm-2 col-form-label">Member</label >
    <div class="col-sm-10">
        <select type="boolean" id="Member" name="member" disabled>
            <option value="">Choose the member</option>
            <option value="true">Manager</option>
            <option value="false">Softaware Developer</option>
            <option value="false">Quality Assurance</option>
          </select>
      
    </div>
  </div>

  <button type="submit" class="btn btn-success">Search</button>
  

  <br /><br /><br /><br />

  <h1>Options</h1>

  <div class="mb-3 row">
    <label for="password" class="col-sm-2 col-form-label">Type</label>
    <div class="col">
      <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
      <label class="form-check-label" for="flexCheckDefault">
        ID
      </label>
    </div>
    <div class="col">
      <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
      <label class="form-check-label" for="flexCheckChecked">
        Name
      </label>
    </div>
</div>
<button type="submit" class="btn btn-success">Apply</button>
  <a class="btn btn-warning" href="/">Reset</a>
  <br /><br /><br /><br />

  
</form>
  

  <table class="table table-striped">
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Action</th>
    </tr>
    <% nama.forEach((item, index) => { %>
    <tr class="genap">
      <td><%= item.projectid %></td>
      <td><%= item.name %></td>
      
      <td>
        <a class="btn btn-success" href="/edit/">edit</a>
        <a class="btn btn-danger" href="/delete/">delete</a>
      </td>
    </tr>
    <% }) %>
  </table>

 
  <a class="btn btn-primary" href="#"> Add</a>

  <script>
    function myFunction1() {
      if (document.getElementById("myCheck1").checked == true) {
        document.getElementById("ID").disabled = false;
      }
      else {
        document.getElementById("ID").disabled = true;
      }
      
    }
    function myFunction2() {
      if (document.getElementById("myCheck2").checked == true) {
        document.getElementById("Name").disabled = false;
      }
      else {
        document.getElementById("Name").disabled = true;
      }
      
    }
    function myFunction3() {
      if (document.getElementById("myCheck3").checked == true) {
        document.getElementById("Member").disabled = false;
      }
      else {
        document.getElementById("Member").disabled = true;
      }
      
    }
    function myFunction4() {
      if (document.getElementById("myCheck4").checked == true) {
        document.getElementById("start").disabled = false;
        document.getElementById("end").disabled = false;
      }
      else {
        document.getElementById("start").disabled = true;
        document.getElementById("end").disabled = true;
      }
      
    }
    function myFunction5() {
      if (document.getElementById("myCheck5").checked == true) {
        document.getElementById("Status").disabled = false;
      }
      else {
        document.getElementById("Status").disabled = true;
      }
      
    }
    </script>

  <%- include('../partials/footer') %>
